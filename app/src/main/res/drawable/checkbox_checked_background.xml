<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="50dp"
                android:height="50dp" />
            <solid android:color="#FFFFFF" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="oval">
            <size
                android:width="50dp"
                android:height="50dp" />
            <solid android:color="@color/md_theme_primary" />
        </shape>
    </item>
    <item
        android:width="50dp"
        android:height="50dp"
        android:bottom="2dp"
        android:drawable="@drawable/checked_icon"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp" />
</layer-list>